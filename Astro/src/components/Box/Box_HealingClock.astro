---
import Clocks_SingleContainer from "../Clocks/Clocks_SingleContainer.astro";

const {id, classes, attr} = Astro.props;
---
<div id={id} class={`gw-1 grid gtw-1 ${classes}`}>
    <div class="b bg-black text-center upcase">Healing</div>
    <div class="pad-left i">Project clock</div>
    <div class="gh-2 justify-center">
        <Clocks_SingleContainer attr={attr} maxSize={4} defaultSize={4}></Clocks_SingleContainer>
    </div>
</div>