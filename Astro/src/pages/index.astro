                        <div class="text-left b">PERK</div>
                        <input type="text" name="attr_soi4contacts" class="gw-2 border-bottom" />
                        <div class="gw-2 text-left b">CONTACTS</div>
                    </div>
                </fieldset>
            </div>
        </div>
        <div id="houseabilitypanel" class="grid gtw-1 panel-centre">
