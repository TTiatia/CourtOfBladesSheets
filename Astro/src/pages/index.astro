---
import Roll20 from '../layouts/Roll20.astro';


---

<Roll20>
    <input type="radio" value="0" class="shape-diamond-med checked-red" name="page-selector" id="page-selector" />
    <input type="radio" value="1" class="shape-dot checked-red" name="page-selector" id="page-selector" />
    <input type="radio" value="2" class="shape-dot checked-red" name="page-selector" id="page-selector" />
    <input type="radio" value="3" class="shape-dot checked-red" name="page-selector" id="page-selector" />
    <input type="radio" value="4" class="shape-dot checked-red" name="page-selector" id="page-selector" checked={true} />
    <div id="page-1" class="page">
        <div id="housedetailpanel" class="panel-left debug">
            <div id="housedetailpanel-titlebox" class="debug grid gh-3 gw-9 grid gtw-1">
                <div class="debug gw-1 gh-3 gapc-2 text-center"><span class="logo">Court of Blades</span></div>
            </div>
            <div id="housedetailpanel-details" class="debug grid gtw-3">
                <input type="text" name="attr_coteriename" class="gw-3 border-bottom pcdetail" />
                <div class="gw-3 b text-left">COTERIE NAME</div>
                <input type="text" name="attr_housestrengths" class="gw-2 border-bottom pcdetail marg-right" />
                <input type="text" name="attr_househealer" class="gw-1 border-bottom pcdetail" />
                <div class="gw-2 b text-left">STRENGTHS</div>
                <span class="gw-1 b marg-right">HEALER TYPE</span>
                <div class="gw-3 i text-left">Force, Intelligence, Magic, Supply, Transport, or Wealth</div>
                <div class="gw-3 grid gtw-3 align-center">
                    <input type="text" name="attr_houserep" class="pcdetail gw-3" />
                    <div class="gw-2 b text-left">REPUTATIONS</div>
                    <div class="gw-1 b text-center">INFLUENCE</div>
                    <div class="gw-2 i">Ambitious, Brutal, Daring, Honorable, Subtle, Arcane, Loyal, Reliable, Uncouth, Respectable, Dangerous</div>
                    <div id="housedetailpanel-influencebox" class="gw-1 tracker tracked-red text-center">
                        <input type="radio" name="attr_houseinfluence" value="0" class="shape-cross checked-hidden track-zero" checked={true} />
                        <input type="radio" name="attr_houseinfluence" value="1" class="shape-diamond-big" />
                        <input type="radio" name="attr_houseinfluence" value="2" class="shape-diamond-big" />
                        <input type="radio" name="attr_houseinfluence" value="3" class="shape-diamond-big" />
                        <input type="radio" name="attr_houseinfluence" value="4" class="shape-diamond-big" />
                        <input type="radio" disabled={true} class="shape-diamond-med shape-hidden" />
                    </div>
                </div>
            </div>
            <div id="housepanel-exposureshame" class="debug gw-9 grid gth-2 gtw-3 text-center padding-top padding-bottom maxh-3">
                <span class="b gw-2 align-bottom">EXPOSURE</span>
                <span class="b gw-1 align-bottom">SHAME</span>
                <div class="gw-2">
                    <div id="housedetailpanel-exposurebox" class="tracker tracked-red">
                        <input type="radio" name="attr_exposure" value="0" class="shape-cross checked-hidden track-zero" checked={true} />
                        <input type="radio" name="attr_exposure" value="1" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="2" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="3" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="4" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="5" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="6" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="7" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="8" class="shape-diamond-big" />
                        <input type="radio" name="attr_exposure" value="9" class="shape-diamond-big" />
                        <input type="radio" disabled={true} class="shape-diamond-big shape-hidden" />
                    </div>
                </div>
                <div id="housedetailpanel-shamebox" class="gw-1 tracker tracked-red">
                    <input type="radio" name="attr_shame" value="0" class="shape-cross checked-hidden track-zero" checked={true} />
                    <input type="radio" name="attr_shame" value="1" class="shape-diamond-big" />
                    <input type="radio" name="attr_shame" value="2" class="shape-diamond-big" />
                    <input type="radio" name="attr_shame" value="4" class="shape-diamond-big" />
                    <input type="radio" disabled={true} class="shape-diamond-med shape-hidden" />
                </div>
            </div>
            <div id="housedetailpanel-soi" class="debug grid gtw-1">
