                        <input type="text" name="attr_soi1name" class="border-bottom" />
                        <input type="text" name="attr_soi1perk" class="border-bottom" />
                        <div class="text-left b">SPHERE OF INFLUENCE</div>
                        <div class="text-left b">PERK</div>
                        <input type="text" name="attr_soi1contacts" class="gw-2 border-bottom" />
